{
  "name": "fake-cloudwatch-logs",
  "version": "1.4.1",
  "description": "Setup a fake Cloudwatch logs server for testing purposes",
  "main": "src/index.js",
  "scripts": {
    "check": "npr tsc -p .",
    "test": "npm run check && npm run lint && npm run unit-test",
    "unit-test": "node test/index.js",
    "lint": "npr tsdocstandard",
    "type-coverage": "npr type-coverage --detail --strict --ignore-catch --at-least 100 --ignore-files=test/lambdas/hello.js"
  },
  "dependencies": {},
