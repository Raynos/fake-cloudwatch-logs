  "devDependencies": {
    "@pre-bundled/rimraf": "3.0.0",
    "@pre-bundled/tape": "4.10.2",
    "@types/node": "12.0.2",
    "aws-sdk": "2.549.0",
    "npm-bin-deps": "1.6.0",
    "tape-cluster": "3.2.1"
  },
  "tsdocstandard": {
    "ignore": [
      "scripts/cache-from-prod.js"
    ]
  },
